AHA;AAIA,QAAA,UAAA,EAAA,OAJA;AAKA,QAAA,WAAA,EAAA,cALA;AAMA,QAAA,SAAA,EAAA;AANA,OAAA;AASA;AAXA,GAxBA;AAsCA,EAAA,OAAA,EAAA;AACA,IAAA,kBADA,gCACA;AAAA;;AACA,MAAA,UAAA,CAAA,YAAA;AAEA,QAAA,MAAA,CAAA,cAAA,GAAA,IAAA;AAIA,OANA,EAMA,GANA,CAAA;AASA,KAXA;AAYA,IAAA,GAZA,iBAYA,CAEA,CAdA;AAeA,IAAA,aAfA,2BAeA;AAEA,WAAA,KAAA,CAAA,eAAA;AACA,KAlBA;AAmBA,IAAA,eAnBA,6BAmBA;AACA,WAAA,cAAA,GAAA,KAAA;AACA,WAAA,KAAA,CAAA,mBAAA,EAAA,KAAA,SAAA;AAEA,KAvBA;AAwBA,IAAA,kBAxBA,gCAwBA;AACA,WAAA,cAAA,GAAA,KAAA;AACA,WAAA,KAAA,CAAA,sBAAA,EAAA,KAAA,SAAA;AACA,KA3BA;AA6BA,IAAA,OA7BA,qBA6BA;AAAA;;AACA,MAAA,UAAA,CAAA,YAAA;AAAA,QAAA,MAAA,CAAA,QAAA,GAAA,IAAA;AAAA,OAAA,EAAA,GAAA,CAAA;AAEA,KAhCA;AAiCA,IAAA,KAjCA,mBAiCA;AACA,WAAA,QAAA,GAAA,CAAA,KAAA,QAAA;AACA,WAAA,UAAA,CAAA,KAAA,GAAA,EAAA;AAEA,KArCA;AAsCA,IAAA,WAtCA,yBAsCA;AACA,UAAA,cAAA,GAAA,IAAA,CAAA,KAAA,CAAA,IAAA,CAAA,SAAA,CAAA,KAAA,UAAA,CAAA,CAAA,CADA,CAEA;;AACA,WAAA,MAAA,CAAA,QAAA,CAAA;AAAA,QAAA,IAAA,EAAA,UAAA;AAAA,QAAA,IAAA,EAAA,cAAA;AAAA,QAAA,SAAA,EAAA,KAAA;AAAA,OAAA;AACA,WAAA,QAAA,GAAA,CAAA,KAAA,QAAA;AACA,WAAA,UAAA,CAAA,KAAA,GAAA,EAAA;AAEA;AA7CA,GAtCA;AAuFA,EAAA,UAAA,EAAA;AACA,IAAA,SAAA,EAAA,SADA;AAEA,IAAA,WAAA,EAAA,WAFA,CAGA;;AAHA;AAvFA,CAAA","sourcesContent":["<template>\r\n  <!-- <div> -->\r\n    <div class=\"task-group flex column closer\">\r\n      <div class=\"closer relative flex space-between align-center\">\r\n        <p class=\"group-title\">{{taskGroup.title}}</p> \r\n        <button class=\"btn-edit closer\" @click=\"openTaskGroupModal\">...</button>\r\n        <div v-if=\"taskModalShown\" class=\"title-modal column \">\r\n           <div @click=\"isAdding = true , taskModalShown = false\"> <i class=\"fas fa-plus\"></i> Add new task </div>\r\n           <div @click=\"duplicateTaskGroup\"><i class=\"fas fa-copy\"></i> Duplicate List</div>\r\n           <div  @click=\"removeTaskGroup\"> <i class=\"far fa-trash-alt\"></i> Remove list</div>\r\n            </div>\r\n      </div>\r\n      <div class=\"tasks1 closer\">\r\n    <!-- <h1> {{this.columns.columnTitle}}</h1> -->\r\n        <draggable   class=\"list-group closer\"\r\n        tag=\"div\"   \r\n        v-bind=\"dragOptions\" v-model=\"taskGroup.tasks\" group=\"people\" @start=\"drag=true\" @end=\"drag=false,updateBoardEv()\">\r\n          <div v-for=\"task in taskGroup.tasks\" :key=\"task.id\">\r\n            <task-preview :task=\"task\"></task-preview>\r\n          </div>\r\n        </draggable>\r\n\r\n        <button v-if=\"!isAdding\" class=\"btn-add-task closer\" @click=\"addTask\">\r\n          <i class=\"fas fa-plus fa-xs\"></i> \r\n          Add another card\r\n        </button>\r\n        <div class=\"add-new-task \" v-if=\"isAdding\">\r\n            <form @submit.prevent=\"saveNewTask\">\r\n               <textarea-autosize\r\n                  class=\"add-textarea\"\r\n                  placeholder=\"Enter a title for this card...\"\r\n                  ref=\"myTextarea\"\r\n                  v-model=\"taskToSave.title\"\r\n                  :min-height=\"30\"\r\n                  :max-height=\"350\"\r\n              />\r\n                  <!-- @focus.native=\"onFocusTextarea\" -->\r\n              <button class=\"btn-save-task\" type=\"submit\">Add Card</button>\r\n            </form>\r\n            <button class=\"btn-close\" @click=\"close\">&times;</button>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  <!-- </div> -->\r\n</template>\r\n\r\n<script>\r\nimport draggable from \"vuedraggable\";\r\nimport taskPreview from \"./taskPreview.vue\";\r\nimport {taskGroupService} from '../services/task-group-service.js'\r\nimport {eventBus} from '../services/event-bus.service'\r\n\r\n// import card from './card.vue'\r\nexport default {\r\n  props: [\"taskGroup\"],\r\n  data() {\r\n    return {\r\n      // taskGroup : this.taskGroup,\r\n      isAdding: false,\r\n      taskModalShown: false ,\r\n      taskToSave: {\r\n        _id : taskGroupService.makeId(),\r\n        title: '',\r\n        bgColor: \"\",\r\n        taskGroup: this.taskGroup._id,\r\n        labels: []\r\n      }\r\n    };\r\n  },\r\n  created() {\r\n    eventBus.$on('closer-clicked', () => {\r\n             \r\n             this.isAdding = false\r\n             this.taskModalShown = false\r\n           })\r\n    \r\n  },\r\n  computed: {\r\n    dragOptions() {\r\n      return {\r\n        animation: 200,\r\n        group: \"description\",\r\n        disabled: false,\r\n        ghostClass: \"ghost\",\r\n        chosenClass: \"chosen-class\",\r\n        dragClass: \"drag-class\"\r\n\r\n      };\r\n    }\r\n  },\r\n\r\n  methods: {\r\n    openTaskGroupModal() {\r\n      setTimeout(()=> {\r\n        \r\n          this.taskModalShown = true\r\n        \r\n        \r\n        \r\n        }, 0.2)\r\n      \r\n      \r\n    },\r\n   log() {\r\n    \r\n   },\r\n   updateBoardEv() {\r\n     \r\n     this.$emit ('updateBoardEv')\r\n   },\r\n   removeTaskGroup(){\r\n    this.taskModalShown = false\r\n     this.$emit ('removeTaskGroupEv' , this.taskGroup)\r\n    \r\n   },\r\n   duplicateTaskGroup() {\r\n     this.taskModalShown = false\r\n     this.$emit ('duplicateTaskGroupEv' , this.taskGroup)\r\n   },\r\n\r\n   addTask() {\r\n     setTimeout(()=> {this.isAdding = true;}, 0.3)\r\n      \r\n    },\r\n    close() {\r\n      this.isAdding = !this.isAdding;\r\n       this.taskToSave.title = ''\r\n     \r\n    },\r\n    saveNewTask() {\r\n      let taskToSaveCopy = JSON.parse(JSON.stringify(this.taskToSave))\r\n      // if (!this.taskToSave) return;\r\n      this.$store.dispatch({ type: 'saveTask', task: taskToSaveCopy, taskGroup: this.taskGroup });\r\n      this.isAdding = !this.isAdding;\r\n      this.taskToSave.title = ''\r\n      \r\n    }\r\n  },\r\n\r\n \r\ncomponents: {\r\n  draggable,\r\n  taskPreview\r\n  // card\r\n}\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n\r\n\r\n.title-modal{\r\n  z-index: 1;\r\n  width: 120px;\r\n  height: 85px;\r\n  position: absolute;\r\n  background-color: rgb(252, 250, 247);\r\n  justify-content: center;\r\n  right: 1%;\r\n  top: 95%;\r\n  div {\r\n    // margin-top: 7px;\r\n    height: 30px;\r\n    cursor: pointer;\r\n  }\r\n      div:hover {\r\n        background-color: rgb(187, 185, 183)\r\n      }\r\n    \r\n}\r\n.fa-trash-alt {\r\n  margin-left: 2px;\r\n  float: left;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/components"}]}