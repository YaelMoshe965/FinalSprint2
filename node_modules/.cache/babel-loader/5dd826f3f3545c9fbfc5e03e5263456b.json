QAAA,CAAA,EAAA;AAEA,aAAA,UAAA,GAAA,KAAA;AACA,QAAA,QAAA,CAAA,KAAA,CAAA,gBAAA;AACA,OALA,CAMA;;AACA,KAPA;;AASA,IAAA,QAAA,CAAA,GAAA,CAAA,gBAAA,EAAA,YAAA;AAEA,MAAA,KAAA,CAAA,UAAA,GAAA,KAAA;AACA,KAHA;AAKA,IAAA,QAAA,CAAA,GAAA,CAAA,WAAA,EAAA,YAAA;AACA,MAAA,KAAA,CAAA,MAAA,CAAA,QAAA,GAAA,IAAA;AACA,KAFA;AAIA,IAAA,QAAA,CAAA,GAAA,CAAA,gBAAA,EAAA,YAAA;AACA,MAAA,KAAA,CAAA,MAAA,CAAA,QAAA,GAAA,KAAA;AACA,KAFA;AAGA,GA1CA;AA4CA;AACA;AACA;AACA;AACA;AACA;AACA,EAAA,OAAA,EAAA;AACA,IAAA,GADA,iBACA,CAEA,CAHA;AAIA;AACA;AACA;AACA,IAAA,UAPA,sBAOA,IAPA,EAOA;AACA,WAAA,MAAA,CAAA,QAAA,CAAA;AAAA,QAAA,IAAA,EAAA,YAAA;AAAA,QAAA,IAAA,EAAA;AAAA,OAAA;AACA,WAAA,IAAA,GAAA,IAAA;AACA,KAVA;AAWA,IAAA,WAXA,uBAWA,KAXA,EAWA;AACA,WAAA,KAAA,CAAA,UAAA,CAAA,OAAA,CAAA,UAAA,aAAA,EAAA;AACA,QAAA,aAAA,CAAA,KAAA,CAAA,OAAA,CAAA,UAAA,IAAA;AAAA,iBAAA,IAAA,CAAA,SAAA,GAAA,aAAA,CAAA,GAAA;AAAA,SAAA;AACA,OAFA;AAIA,WAAA,MAAA,CAAA,QAAA,CAAA;AAAA,QAAA,IAAA,EAAA,aAAA;AAAA,QAAA,KAAA,EAAA;AAAA,OAAA;AACA,KAjBA;AAkBA,IAAA,UAlBA,sBAkBA,IAlBA,EAkBA;AACA,WAAA,MAAA,CAAA,QAAA,CAAA;AAAA,QAAA,IAAA,EAAA,YAAA;AAAA,QAAA,IAAA,EAAA;AAAA,OAAA;AACA,KApBA;AAqBA,IAAA,eArBA,2BAqBA,KArBA,EAqBA;AACA,WAAA,UAAA,GAAA,KAAA;AACA,WAAA,MAAA,CAAA,QAAA,CAAA;AAAA,QAAA,IAAA,EAAA,cAAA;AAAA,QAAA,KAAA,EAAA;AAAA,OAAA;AAEA,KAzBA;AA0BA,IAAA,kBA1BA,8BA0BA,SA1BA,EA0BA;AACA,WAAA,MAAA,CAAA,QAAA,CAAA;AAAA,QAAA,IAAA,EAAA,oBAAA;AAAA,QAAA,SAAA,EAAA;AAAA,OAAA;AACA,KA5BA;AA8BA,IAAA,eA9BA,2BA8BA,SA9BA,EA8BA;AACA,WAAA,MAAA,CAAA,QAAA,CAAA;AAAA,QAAA,IAAA,EAAA,iBAAA;AAAA,QAAA,SAAA,EAAA;AAAA,OAAA;AACA;AAhCA;AAlDA,sDAqFA;AACA,EAAA,WADA,yBACA;AACA,WAAA;AACA,MAAA,SAAA,EAAA,GADA;AAEA,MAAA,KAAA,EAAA,aAFA;AAGA,MAAA,QAAA,EAAA,KAHA;AAIA,MAAA,UAAA,EAAA,OAJA;AAKA,MAAA,WAAA,EAAA,cALA;AAMA,MAAA,SAAA,EAAA;AANA,KAAA;AAQA;AAVA,CArFA,wDAiGA;AACA,EAAA,SAAA,EAAA,SADA;AAEA,EAAA,SAAA,EAAA,SAFA;AAGA,EAAA,WAAA,EAAA,WAHA;AAIA,EAAA,QAAA,EAAA;AAJA,CAjGA","sourcesContent":["<template>\r\n<section class=\"column\" v-if=\"board\" :class=\"{screen: screen.isScreen}\">\r\n  <!-- :class=\"{screen: screen.isScreen}\" -->\r\n  <!-- <div @click=\"log\" class=\"overlay\"> test</div> -->\r\n       <task-edit @removeTaskEv='removeTask'/>\r\n       <task-details  @updateTaskEv='updateTask' @removeTaskEv='removeTask'> </task-details>\r\n      <div >\r\n <div class=\"flex closer\">\r\n    <draggable   class=\"list-group closer flex flex-start\"\r\n        tag=\"div\"   \r\n        v-bind=\"dragOptions\" v-model=\"board.taskGroups\" group=\"columns\" @start=\"drag=true\" @end=\"drag=false , updateBoard(board)\">\r\n       <div v-for=\"taskGroup in board.taskGroups\" :key='taskGroup.id'>\r\n       <task-group @duplicateTaskGroupEv='duplicateTaskGroup' @removeTaskGroupEv='removeTaskGroup' @updateBoardEv='updateBoard(board)' :taskGroup='taskGroup'> </task-group>\r\n       </div>\r\n    </draggable>\r\n  <div @click=\"addingTask = true\" class=\"task-add flex justify-center align-center closer\"> \r\n    <div @click=\"addingTask = true\" class=\"closer\" v-if=\"!addingTask\">\r\n    Add task group\r\n    </div>\r\n    <div class=\"add-group-inputs flex column \" v-if=\"addingTask\">\r\n\r\n    <input  placeholder=\"Enter a title\" type=\"text\" v-model=\"newGroupTitle\"> \r\n    <div>\r\n    <button class=\"btn-save-group\" @click.stop=\"createTaskGroup(newGroupTitle),newGroupTitle = '' \">Add Group</button>\r\n    <button class=\"btn-close\" @click.stop=\"addingTask = !addingTask\"><i class=\"fas fa-times fa-lg\"></i></button>\r\n    </div>\r\n\r\n    </div>\r\n     </div>\r\n  </div>\r\n  </div> \r\n  </section>\r\n</template>\r\n\r\n\r\n\r\n<script>\r\nimport draggable from 'vuedraggable'\r\nimport taskGroup from '../components/taskGroup.vue'\r\nimport taskDetails from '../components/taskDetails.vue'\r\nimport taskEdit from '../components/taskEdit.vue';\r\nimport {taskGroupService} from '../services/task-group-service.js'\r\n// import {boardService} from '../services/board-service.js'\r\nvar boardService = require('../services/board-service.js');\r\nimport {eventBus, SCREEN_MODE, STOP_SCREEN_MODE} from '../services/event-bus.service'\r\n\r\n\r\n\r\nexport default {\r\n  data(){\r\n    return {\r\n      board :  null,\r\n      newGroupTitle: '',\r\n      addingTask: false,\r\n      screen: {\r\n        isScreen: false\r\n      }\r\n    }\r\n  },\r\n  computed: {\r\n    closeAddMenu(){\r\n      return this.addingTask = false\r\n    // this.$store.getters.currBoard\r\n    }\r\n  },\r\n\r\n  created() {\r\n      this.$store.dispatch({ type: 'loadBoard' })\r\n        .then(board => this.board = board);\r\n        window.onclick = function(ev) {\r\n          if ( ev.target.classList.contains(\"closer\")){\r\n           \r\n           this.addingTask = false\r\n           eventBus.$emit('closer-clicked')\r\n          }\r\n          // console.log('window clickied', ev.target.classList[0]);\r\n           }\r\n\r\n           eventBus.$on('closer-clicked', () => {\r\n            \r\n             this.addingTask = false\r\n           })\r\n\r\n      eventBus.$on(SCREEN_MODE, () => {\r\n          this.screen.isScreen = true;\r\n      })\r\n\r\n       eventBus.$on(STOP_SCREEN_MODE, () => {\r\n          this.screen.isScreen = false;\r\n      })\r\n },\r\n\r\n  // $watch() {\r\n  //      eventBus.$on(SCREEN_MODE, () => {\r\n  //        console.log('hello');\r\n  //         this.screen.isScreen = true;\r\n  //     })\r\n  // },\r\n methods: {\r\n   log() {\r\n   \r\n   },\r\n  //  updateBoard(){\r\n  //    console.log('update trigger');\r\n  //  },\r\n   removeTask(task) {\r\n     this.$store.dispatch({ type: 'removeTask', task })\r\n     this.task = null\r\n   },\r\n   updateBoard(board) {\r\n     this.board.taskGroups.forEach( taskGroupItem => {\r\n       taskGroupItem.tasks.forEach(task => task.taskGroup = taskGroupItem._id )\r\n     } )\r\n     \r\n     this.$store.dispatch({ type: 'updateBoard', board })\r\n   },\r\n   updateTask(task){\r\n         this.$store.dispatch({ type: 'updateTask', task: task })\r\n   },\r\n   createTaskGroup(title) {\r\n     this.addingTask = false\r\n     this.$store.dispatch({ type: 'addTaskGroup', title })\r\n     \r\n   },\r\n   duplicateTaskGroup(taskGroup){\r\n      this.$store.dispatch({ type: 'duplicateTaskGroup', taskGroup })\r\n   },\r\n\r\n   removeTaskGroup(taskGroup){\r\n     this.$store.dispatch({ type: 'removeTaskGroup', taskGroup })\r\n   }\r\n\r\n },\r\n    computed: {\r\n    dragOptions() {\r\n      return {\r\n        animation: 200,\r\n        group: \"description\",\r\n        disabled: false,\r\n        ghostClass: \"ghost\",\r\n        chosenClass: \"chosen-class\",\r\n        dragClass: \"drag-class\"\r\n      }\r\n    }\r\n  },\r\ncomponents: {\r\n  taskGroup,\r\n  draggable,\r\n  taskDetails,\r\n  taskEdit\r\n}\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" >\r\n\r\n\r\n .btn-save-group {\r\n      background-color: #5aac44;\r\n      color: #fbfdfb;\r\n      font-size: 0.9rem;\r\n      padding: 10px;\r\n      border-radius: 3px;\r\n      float: left;\r\n      margin-left: 10px;\r\n      margin-bottom: 10px;\r\n      border: 0;\r\n      cursor: pointer;\r\n      outline: none;\r\n\r\n      &:hover {\r\n        background-color: #61bd4f;\r\n      }\r\n      }\r\n        .add-group-inputs {\r\n\r\n          margin-top: 10px ;\r\n          justify-content: center;\r\n          align-items: center;\r\n          .btn-close {\r\n        float: left;\r\n        margin-left: 10px;\r\n        margin-top: 10px;\r\n        font-size: 16px;\r\n        border: 0;\r\n        cursor: pointer;\r\n        outline: none;\r\n        width: 30px;\r\n        background: rgba($color: #cfcdcd, $alpha: 0.1);\r\n  \r\n        &:hover {\r\n          color: #172b4d;\r\n          // background-color: #ebecf0;\r\n        }\r\n      }\r\n           input {\r\n                   outline: 0px;\r\n              border: 0px;\r\n              background-color: #f6f6f6;\r\n        }\r\n        div {\r\n          margin-top: 5px;\r\n        }\r\n      \r\n    }\r\n</style>\r\n"],"sourceRoot":"src/views"}]}